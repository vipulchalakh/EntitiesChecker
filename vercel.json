{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/entities",
            "dest": "main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 