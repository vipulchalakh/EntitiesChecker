{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "250mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/entities",
            "dest": "main.py"
        }
    ]
} 