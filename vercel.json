{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt",
    "outputDirectory": ".",
    "framework": "fastapi",
    "routes": [
        {
            "src": "/entities",
            "dest": "main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 